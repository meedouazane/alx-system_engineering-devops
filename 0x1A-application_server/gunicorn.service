[Unit]
Description=Hbnb_Start
After=network.target

[Service]
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4/
ExecStart=/home/ubuntu/AirBnB_clone_v4/myproject/bin/gunicorn --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app --workers 3 --error-logfile /tmp/airbnb-api-error.log --access-logfile /tmp/airbnb-access.log
Restart=always
User=ubuntu

[Install]
WantedBy=multi-user.target
